highway=footway & mtb:scale=2 & bad_vis=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10611 road_class=0 resolution 23-24]
highway=footway & mtb:scale=2 & bad_vis=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width<=0.5) 				[0x10611 road_class=0 resolution 24]

highway=footway & mtb:scale=3 & bad_vis=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10612 road_class=0 resolution 23-24]
