highway=track & mtb:scale=0 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes							  		[0x326 road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=0 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes							  		[0x327 road_class=0 resolution 23-24]

