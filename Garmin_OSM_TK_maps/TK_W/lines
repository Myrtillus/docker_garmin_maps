# -------------------------------------------------------------------
# This is the default mkgmap style file for converting OSM ways
# into Garmin lines.
# Any line prefaced with # is a comment and will not be applied.
# Each line is a rule, of the generalised format:
#  OSM_tag(s)_to_match  [Garmin_type_code resolution]
# See http://wiki.openstreetmap.org/wiki/Mkgmap/help/style_rules
# and http://wiki.openstreetmap.org/wiki/Mkgmap/help/Custom_styles
# for more information.


##############################################################################################################################
### skiing tracks forbidden for winter
##############################################################################################################################

mtb:winter="-1" {set mtb:winter=no}
highway=* & mtb:winter=no & (ski=designated | piste:difficulty=* | piste:grooming=* | piste:lit=* | piste:oneway=* | piste:priority=* | piste:type=*) {deletealltags; set highway=piste}
highway=piste [0x10F06 resolution 23-24]

##################################################################
### Spellcheck
##################################################################

### Convert tag combinations to suitable forms, adapted from OpenMTBMap
highway=service_footway {set highway=footway}
highway=footpath {set highway=footway}
highway="track; footway" {set highway=footway}
highway="track;footway" {set highway=footway}
highway="path;track" {set highway=path}
highway="path; track" {set highway=path}
highway="track; path" {set highway=track}
highway="track;path" {set highway=track}
highway="footway; track" {set highway=footway}
highway="footway;track" {set highway=footway}
highway="residential; track" {set highway=track}
highway="residential;track" {set highway=track}
highway=residental {set highway=residential}
highway="footway; cycleway" {set highway=cycleway}
highway="footway;cycleway" {set highway=cycleway}
highway="cycleway; footway" {set highway=cycleway}
highway="cycleway;footway" {set highway=cycleway}
highway=cycleroad {set highway=cycleway}
highway=cyclestreet {set highway=cycleway}
bicycle=nope {set bicycle=no}
bicycle=designated {set bicycle=yes}
highway=pedestrian & area!=yes & bicycle!=no {set highway=cycleway}
bicycle=designated & highway=* {set highway=cycleway}
cycleway=track {set highway=cycleway}

### Convert used tags to lowercase
Highway=* & highway!=* {set highway='${Highway}'}
HighWay=* & highway!=* {set highway='${HighWay}'}
highway=Footway {set highway=footway}
highway=Cycleway {set highway=cycleway}
Mtb:scale=* & mtb:scale=no {set mtb:scale='${Mtb:scale}'}
MTB:scale=* & mtb:scale=no {set mtb:scale='${MTB:scale}'}
MTB:Scale=* & mtb:scale=no {set mtb:scale='${MTB:Scale}'}
Mtb:Scale=* & mtb:scale=no {set mtb:scale='${Mtb:Scale}'}

### Convert random typos
higwhay=* & highway!=* {set highway='${higwhay}'}
higwhey=* & highway!=* {set highway='${higwhey}'}
highwey=* & highway!=* {set highway='${highwey}'}
mbt:scale=* & mtb:scale=no {set mtb:scale='${mbt:scale}'}
hiway=* & highway!=* {set highway='${hiway}'}
footwey=* & highway!=* {set footway='${footwey}'}
fotway=* & highway!=* {set footway='${fotway}'}
highway=residental {set highway=residential}
visibility=* & trail_visibility!=* {set trail_visibility='${visibility}'}
note=* {delete note}

### Commas to periods, removing incorrectly placed "m" (should have a space)
width="0,1" {set width="0.1"}
width="0,2" {set width="0.2"}
width="0,3" {set width="0.3"}
width="0,4" {set width="0.4"}
width="0,5" {set width="0.5"}
width="0,6" {set width="0.6"}
width="0,7" {set width="0.7"}
width="0,8" {set width="0.8"}
width="0,9" {set width="0.9"}
width="1,0" {set width="1.0"}

width="0,1m" {set width="0.1"}
width="0,2m" {set width="0.2"}
width="0,3m" {set width="0.3"}
width="0,4m" {set width="0.4"}
width="0,5m" {set width="0.5"}
width="0,6m" {set width="0.6"}
width="0,7m" {set width="0.7"}
width="0,8m" {set width="0.8"}
width="0,9m" {set width="0.9"}
width="1,0m" {set width="1.0"}

width="0.1m" {set width="0.1"}
width="0.2m" {set width="0.2"}
width="0.3m" {set width="0.3"}
width="0.4m" {set width="0.4"}
width="0.5m" {set width="0.5"}
width="0.6m" {set width="0.6"}
width="0.7m" {set width="0.7"}
width="0.8m" {set width="0.8"}
width="0.9m" {set width="0.9"}
width="1.0m" {set width="1.0"}

###################################################################
### highway conversions, simplification etc.
###################################################################

highway=construction {deletealltags; add highway=construction} [0x10D13 resolution 22-24]
highway=pedestrian & bicycle!=no {set highway=path}
highway=bridleway & bicycle!=no {set highway=path}
highway=living_street {set highway=residential}
highway=bicycle_road {set highway=cycleway}
bicycle=yes & mtb:scale=no {set highway=cycleway}
mtb:scale="0-" | mtb:scale="0+" {set mtb:scale=0}
mtb:scale="1-" | mtb:scale="1+" {set mtb:scale=1}
mtb:scale="2-" | mtb:scale="2+" {set mtb:scale=2}
mtb:scale="3-" | mtb:scale="3+" {set mtb:scale=3}
mtb:scale="4-" | mtb:scale="4+" {set mtb:scale=4}
mtb:scale="5-" | mtb:scale="5+" {set mtb:scale=5}
highway=* & mtb:scale!=* {set mtb:scale=no}
trail_visibility=bad | trail_visibility=horrible | trail_visibility=no {delete trail_visibility; set bad_vis=yes}
surface!=mud {delete surface}

###################################################################
### Cycleways
###################################################################

highway=cycleway & (mtb:scale=no | mtb:scale=0) [0x10119 road_class=0 resolution 23-24 continue]
highway=cycleway & (mtb:scale=no | mtb:scale=0) [0x10E16 road_class=0 resolution 20-22]

# Cycleways with mtb:scale higher than 0 treated as highway=track
highway=cycleway & (mtb:scale=1 | mtb:scale=2 | mtb:scale=3 | mtb:scale=4 | mtb:scale=5) {set highway=track}

###################################################################
### Footpaths not permitted for bicycles
###################################################################

highway=pedestrian & area!=yes & bicycle=no & mtb:scale!=* {set highway=nocycle}
# highway=footway & bicycle=no & mtb:scale=no {set highway=nocycle}
# highway=footpath & bicycle=no & mtb:scale=no {set highway=nocycle}
highway=path & bicycle=no & mtb:scale=no {set highway=nocycle}
highway=track & bicycle=no & mtb:scale=no {set highway=nocycle}
highway=sidewalk {set highway=nocycle}
# highway=footway & (foot=designated | foot=yes) & bicycle!=yes & mtb:scale=no {set highway=nocycle}
# highway=footpath & (foot=designated | foot=yes) & bicycle!=yes & mtb:scale=no {set highway=nocycle}
highway=path & (foot=designated | foot=yes) & bicycle=no & mtb:scale=no {set highway=nocycle}
highway=track & (foot=designated | foot=yes) & bicycle=no & mtb:scale=no {set highway=nocycle}

### Simplified footway rules, everything without bicycle designation or mtb:scale marked as footway.
highway=footway & bicycle!=yes & mtb:scale=no {set highway=nocycle}
highway=footpath & bicycle!=yes & mtb:scale=no {set highway=nocycle}

highway=nocycle [0x10118 road_class=0 resolution 23-24]


#########################################################################################################################################################################################################
### mtb:scale styles for paths/footways/tracks
#########################################################################################################################################################################################################
###Paths colored based on mtb:scale or cycleway key. Solid color lines for zoomed out view to reduce clutter, point is to see what kind of paths are (/have been tagged) where

### Forbidden paths/tracks/footways, must be before general mtb:scale rules (?). Disappear from view when zoomed out.
highway=path & mtb:scale="-1" [0x10E10 resolution 23-24]
highway=track & mtb:scale="-1" [0x10E11 resolution 23-24]
highway=footway & mtb:scale="-1" [0x10E10 resolution 23-24]


##############################################################################################################################
### dead end paths, noexit=yes
##############################################################################################################################

### paths: farzoom, nearzoom, narrow
highway=path & noexit=yes & (width>0.5 | width!=*) 		[0x10D0A road_class=0 resolution 18-22 continue]
highway=path & noexit=yes & (width>0.5 | width!=*) 		[0x10D0B road_class=0 resolution 23-24]
highway=path & noexit=yes & (width<=0.5) 				      [0x10D0A road_class=0 resolution 24]

### footways: farzoom, nearzoom, narrow
highway=footway & noexit=yes & (width>0.5 | width!=*) [0x10D0A road_class=0 resolution 18-22 continue]
highway=footway & noexit=yes & (width>0.5 | width!=*) [0x10D0B road_class=0 resolution 23-24]
highway=footway & noexit=yes & (width<=0.5) 				  [0x10D0A road_class=0 resolution 24]

### tracks: farzoom, nearzoom
highway=track & noexit=yes						 	            	[0x10D0C road_class=0 resolution 18-22 continue]
highway=track & noexit=yes					               		[0x10D0C road_class=0 resolution 23-24]
  

##############################################################################################################################
### no bad_vis=yes, no surface=mud, no obstacle=vegetation
##############################################################################################################################

### paths: farzoom, nearzoom, narrow
highway=path & mtb:scale=no & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10D03 road_class=0 resolution 18-22 continue]
highway=path & mtb:scale=no & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E13 road_class=0 resolution 23-24]
highway=path & mtb:scale=no & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width<=0.5) 				[0x10D03 road_class=0 resolution 24]

highway=path & mtb:scale=0 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E18 road_class=0 resolution 18-22 continue]
highway=path & mtb:scale=0 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E02 road_class=0 resolution 23-24]
highway=path & mtb:scale=0 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width<=0.5) 					[0x10F00 road_class=0 resolution 24]

highway=path & mtb:scale=1 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E19 road_class=0 resolution 18-22 continue]
highway=path & mtb:scale=1 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E03 road_class=0 resolution 23-24]
highway=path & mtb:scale=1 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width<=0.5) 					[0x10F01 road_class=0 resolution 24]

highway=path & mtb:scale=2 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E1A road_class=0 resolution 18-22 continue]
highway=path & mtb:scale=2 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E04 road_class=0 resolution 23-24]
highway=path & mtb:scale=2 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width<=0.5) 					[0x10F02 road_class=0 resolution 24]

highway=path & mtb:scale=3 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E1B road_class=0 resolution 18-22 continue]
highway=path & mtb:scale=3 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E05 road_class=0 resolution 23-24]
highway=path & mtb:scale=3 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width<=0.5) 					[0x10F03 road_class=0 resolution 24]

highway=path & mtb:scale=4 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E1C road_class=0 resolution 18-22 continue]
highway=path & mtb:scale=4 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E06 road_class=0 resolution 23-24]
highway=path & mtb:scale=4 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width<=0.5) 					[0x10F04 road_class=0 resolution 24]

highway=path & mtb:scale=5 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E1D road_class=0 resolution 18-22 continue]
highway=path & mtb:scale=5 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 		[0x10E07 road_class=0 resolution 23-24]
highway=path & mtb:scale=5 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width<=0.5) 					[0x10F05 road_class=0 resolution 24]


### footways: farzoom, nearzoom, narrow
highway=footway & mtb:scale=no & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 	[0x10D03 road_class=0 resolution 18-22 continue]
highway=footway & mtb:scale=no & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 	[0x10E13 road_class=0 resolution 23-24]
highway=footway & mtb:scale=no & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width<=0.5) 				[0x10D03 road_class=0 resolution 24]

highway=footway & mtb:scale=0 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes & (width>0.5 | width!=*) 	[0x10E18 road_class=0 resolution 18-22 continue]
