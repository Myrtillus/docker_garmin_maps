###tracks: farzoom, nearzoom
highway=track & mtb:scale=no & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						 	 	[0x10D03 road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=no & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						 		[0x10D02 road_class=0 resolution 23-24]

highway=track & mtb:scale=0 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E18 road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=0 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E0A road_class=0 resolution 23-24]

highway=track & mtb:scale=1 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E19 road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=1 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E0B road_class=0 resolution 23-24]

highway=track & mtb:scale=2 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E1A road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=2 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E0C road_class=0 resolution 23-24]

highway=track & mtb:scale=3 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E1B road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=3 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E0D road_class=0 resolution 23-24]

highway=track & mtb:scale=4 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E1C road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=4 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E0E road_class=0 resolution 23-24]

highway=track & mtb:scale=5 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E1D road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=5 & bad_vis!=yes & surface!=mud & obstacle!=vegetation & noexit!=yes						  		[0x10E0F road_class=0 resolution 23-24]


