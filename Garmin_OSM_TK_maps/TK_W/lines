highway=track & mtb:scale=1 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes						  			[0x329 road_class=0 resolution 23-24]

highway=track & mtb:scale=2 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes						  			[0x330 road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=2 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes						  			[0x331 road_class=0 resolution 23-24]

highway=track & mtb:scale=3 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes						  			[0x332 road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=3 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes						  			[0x333 road_class=0 resolution 23-24]

highway=track & mtb:scale=4 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes						  			[0x334 road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=4 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes						  			[0x335 road_class=0 resolution 23-24]

highway=track & mtb:scale=5 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes						  			[0x336 road_class=0 resolution 18-22 continue]
highway=track & mtb:scale=5 & bad_vis=yes & surface=mud & obstacle=vegetation & noexit!=yes						  			[0x337 road_class=0 resolution 23-24]







# Name unnamed paths/tracks etc. based on their highwaytype or based on their mtb:scale if it exists
highway=path & mtb:scale=no & name!=* {add name=Path}
highway=path & mtb:scale=* & name!=* {add name='Path, mtb:scale ${mtb:scale}'}
highway=track & mtb:scale=no & name!=* {add name=Track}
highway=track & mtb:scale=* & name!=* {add name='Track, mtb:scale ${mtb:scale}'}
highway=footway & mtb:scale=no & name!=* {add name=Footway}
highway=footway & mtb:scale=* & name!=* {add name='Footway, mtb:scale ${mtb:scale}'}
highway=cycleway & mtb:scale=no & name!=* {add name=Cycleway}
highway=cycleway & mtb:scale=* & name!=* {add name='Cycleway, mtb:scale ${mtb:scale}'}
# Cycleways don't currently have mtb:scales, but in case that changes in the future.


###################################################################
### Highway links and roundabouts
###################################################################

### Links
highway=motorway_link [0x10F10 road_class=3 resolution 0-24]
highway=trunk_link [0x10F11 road_class=3 resolution 0-24]
highway=primary_link [0x10F12 road_class=2 resolution 22-24]
highway=secondary_link [0x10F13 road_class=2 resolution 22-24]
highway=tertiary_link [0x10F14 road_class=2 resolution 22-24]
highway=residential_link [0x10F14 road_class=1 resolution 22-24]
highway=service_link [0x10F14 road_class=0 resolution 22-24]

### Roundabouts
highway=motorway & junction=roundabout [0x10D04 road_class=3 resolution 0-24]
highway=trunk & junction=roundabout [0x10D05 road_class=3 resolution 0-24]
highway=primary & junction=roundabout [0x10D06 road_class=2 resolution 22-24]
highway=secondary & junction=roundabout [0x10D07 road_class=2 resolution 22-24]
highway=tertiary & junction=roundabout [0x10D08 road_class=2 resolution 22-24]
highway=residential & junction=roundabout [0x10D09 road_class=1 resolution 22-24]
highway=service & junction=roundabout [0x10D10 road_class=0 resolution 22-24]

###################################################################
### Alternative linestyles for roads when zoomed out
###################################################################

highway=motorway [0x01 resolution 24 road_class=3 continue]
highway=motorway [0x10F10 resolution 0-23 road_class=3]
highway=trunk [0x02 resolution 24 road_class=3 continue]
highway=trunk [0x10F11 resolution 0-23 road_class=3]
highway=primary [0x03 resolution 24 road_class=2 continue]
highway=primary [0x10F12 resolution 18-23 road_class=2]
highway=secondary [0x04 resolution 24 road_class=2 continue]
highway=secondary [0x10F13 resolution 18-23 road_class=2]
highway=tertiary [0x05 resolution 24 road_class=2 continue]
highway=tertiary [0x10F14 resolution 18-23 road_class=2]
highway=residential [0x06 resolution 24 road_class=1 continue]
highway=residential [0x10F15 resolution 20-23 road_class=1]
highway=service & service!=driveway & service!=alley & service!=emergency_access & service!=drive-through & service!=parking_aisle [0x07 resolution 24 road_class=0 continue]
highway=service & service!=driveway & service!=alley & service!=emergency_access & service!=drive-through & service!=parking_aisle [0x10D10 resolution 21-23 road_class=0]
highway=unclassified [0x07 resolution 24 road_class=0 continue]
highway=unclassified [0x10F16 resolution 21-23 road_class=0]
# unclassified roads rendered the same as serviceroads

###################################################################
### Different kinds of service roads
###################################################################

highway=service & service=driveway [0x10D11 resolution 23 road_class=0]
highway=service & service=alley [0x10D11 resolution 23 road_class=0]
highway=service & service=emergency_access [0x10D11 resolution 23 road_class=0]
highway=service & service=drive-through [0x10D11 resolution 23 road_class=0]
highway=service & service=parking_aisle [0x10D12 resolution 24]
highway=construction [0x10E15 resolution 22-24]

###################################################################
### Walls, fences and hedges
###################################################################

barrier=wall | barrier=city_wall [0x10104 resolution 24]
barrier=retaining_wall [0x10103 resolution 22]
barrier=fence | barrier=cable_barrier | barrier=guard_rail | barrier=handrail [0x1010D resolution 24]
barrier=hedge [0x10200 resolution 24]

###################################################################
### Natural features, powerlines etc. misc.
###################################################################

natural=coastline [0x1011A resolution 12]
natural=cliff [0x1011B resolution 21]
waterway=stream [0x1011C resolution 23-24]
power=line [0x29 resolution 21]
power=minor_line [0x02A resolution 23-24]
# Railway lines, note that many devices display type 0x14 only at resolution 24 
(railway=rail | railway=tram | railway=subway | railway=narrow_gauge | railway=light_rail | railway=preserved) & !(tunnel=yes) [0x10010 resolution 18]
railway=abandoned | railway=disused [0x10011 road_class=0 resolution 18]
railway=platform [0x10010 resolution 18]
highway=steps [0x08 resolution 23-24]


###################################################################
### Hide unnecessary lines i.e. remove their tags
###################################################################

route=ferry {deletealltags}
waterway=river {deletealltags}



addr:housenumber=* {set mkgmap:execute_finalize_rules=true}
aeroway=runway [0x27 resolution 20]
aeroway=taxiway [0x27 resolution 24]

# Assign the street name for house number search
highway=* & name=* { set mkgmap:street='${name}' }

# Mark highways with the toll flag
highway=* & (toll=yes|toll=true) { set mkgmap:toll=yes }

# Hide proposed ways
(highway=proposed | highway=proposal | highway=planned | highway ~ '.*proposed.*') {delete highway;delete junction}
# Hide removed ways
(highway=razed | highway=dismantled) {deletealltags}
# Hide abandoned ways. Abandoned highways have some evidence of their former existence but are no longer used. These
# abandoned highways could be useful in topographical maps.
# https://wiki.openstreetmap.org/wiki/Key:abandoned:
((abandoned:highway=* & highway!=*) | highway=abandoned) {deletealltags}
# Hide other non-existent ways
(highway=unbuilt | highway=neverbuilt | highway=rejected | highway ~ 'x-.*') {delete highway;delete junction}
# Remove highway tag from ways which are not suitable for routing
highway=traffic_signals | highway=junction | highway=island | highway=centre_line | highway=traffic_island  {delete highway} 
highway=no | highway=none {delete highway}
 
# Hide unaccessible tunnels
highway=* & tunnel=yes & (access=private|access=no)
& foot!=* & bicycle!=* {delete highway;delete junction}
# Disable dead-end-checks for unaccessible oneways
highway=* & oneway=yes & (access=private|access=no)
{add mkgmap:dead-end-check=false}
# Validation-like checks (uncomment to enable)
#highway=motorway_link & oneway!=yes & oneway!=no { echo "motorway_link lacks oneway" }
highway=motorway|highway=motorway_link { add oneway=yes; add mkgmap:numbers=false }

# Set highway names to include the reference if there is one
highway=motorway { name '${ref|highway-symbol:hbox} ${name}' | '${ref|highway-symbol:hbox}' | '${name}' }

# start of rules for process-exits and process-destination options
# which may add info to a part of these highway=*_link roads:
# motorway_link, trunk_link, primary_link, secondary_link, tertiary_link
# build destination hint 
mkgmap:dest_hint=*
  { set dest_hint = '${destination:ref|subst: =>} ${mkgmap:dest_hint|subst:;=> |subst:/=> }' |
         '${ref|subst: =>} ${mkgmap:dest_hint|subst:;=> |subst:/=> }' | 
         '${mkgmap:dest_hint|subst:;=> |subst:/=> }';
       }
# build exit hint 
mkgmap:exit_hint=true 
  { set exit_hint = 'Exit ${mkgmap:exit_hint_ref} ${mkgmap:exit_hint_name}' | 
         'Exit ${mkgmap:exit_hint_ref} ${mkgmap:exit_hint_exit_to}' | 
         'Exit ${mkgmap:exit_hint_exit_to}' |
         'Exit ${mkgmap:exit_hint_name}' |
         'Exit ${mkgmap:exit_hint_ref}';
       }
  
# use destination hint and/or exit hint to build name              
(mkgmap:exit_hint=true | mkgmap:dest_hint=*)
  {	name '${exit_hint} ${dest_hint}' | 	'${dest_hint}' | 		'${exit_hint}' }
# end of rules for process-exits and process-destination options	
  
highway=trunk {name '${ref|highway-symbol:hbox} ${name}' | '${ref|highway-symbol:hbox}' | '${name}'; addlabel '${name} (${ref})' }
highway=primary {name '${ref|highway-symbol:box} ${name}' | '${ref|highway-symbol:box}' | '${name}'; addlabel '${name} (${ref})' }
highway=secondary | highway=tertiary {name '${ref|highway-symbol:oval} ${name}' | '${ref|highway-symbol:oval}' | '${name}'; addlabel '${name} (${ref})' }
highway=* {name '${name}' | '${ref}' }

# Flag unpaved roads.
highway=*
& (surface=cobblestone | surface=compacted | surface=dirt |
   surface=earth | surface=grass | surface=grass_paver |
   surface=gravel | surface=grit | surface=ground | surface=mud |
   surface=pebblestone | surface=sand | surface=unpaved |
   mtb:scale=* |
   tracktype ~ 'grade[2-6]' |
   smoothness ~ '.*(bad|horrible|impassable)' |
   sac_scale ~ '.*(mountain|alpine)_hiking' |
   sport=via_ferrata)
{ add mkgmap:unpaved=1 }
(highway=bridleway | highway=path | highway=track | highway=unsurfaced)
& surface!=* & tracktype!=* & smoothness!=* & sac_scale!=*
{ add mkgmap:unpaved=1 }

leisure=track & area!=yes
{add highway=footway; name '${name} (${sport})' | '${name}'}
(man_made=pier | man_made=piste:halfpipe) & area!=yes
{add highway=footway; name '${ref} ${name}' | '${ref}' | '${name}' }





(man_made=cable|(man_made=* & man_made ~ '.*pipe.*')) & area!=yes &
tunnel!=yes & location != underground
{name '${name} (${operator})' | '${name}' | '${operator}' }
[0x28 resolution 23]

# The following boundary styles are after the highway rules because ways
# are frequently tagged with both and we want the highway to take priority.
boundary=administrative { name '${mkgmap:boundary_name}' }
boundary=administrative & admin_level<3 [0x1e resolution 12]
boundary=administrative & admin_level<5 [0x1d resolution 19]
boundary=administrative & admin_level<7 [0x1c resolution 21]
boundary=administrative & admin_level<9 [0x1c resolution 22]
boundary=administrative [0x1c resolution 22]
boundary=national [0x1e resolution 17]
boundary=political [0x1c resolution 19]

include 'inc/water_lines';
include 'inc/contour_lines';







<finalize>
# The finalizer section is executed for each element when a rule with an element type matches

include 'inc/address';
# calculate the road speed based on maxspeed tag
include 'inc/roadspeed'; 
# calculate the access rules
include 'inc/access';

# don't add house numbers to unnamed or artifical bicycle ways
mkgmap:bicycle=yes & (mkgmap:foot=no & mkgmap:car=no & mkgmap:street!=* | mkgmap:synthesised=yes) {set mkgmap:numbers=false}

name=* { name '${name}' }

highway=* & ref=* { addlabel '${ref}' }
highway=* & int_ref=* { addlabel '${int_ref}' }
highway=* & nat_ref=* { addlabel '${nat_ref}' }
highway=* & reg_ref=* { addlabel '${reg_ref}' }


